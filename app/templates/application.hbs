<div class="off-canvas-wrap" data-offcanvas>
  <div class="inner-wrap">

    <!-- Off Canvas Menu -->
    {{partial 'partials/menu'}}

    <!-- Top Bar -->
    {{partial 'partials/topbar'}}
    
